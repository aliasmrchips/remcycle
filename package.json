{
  "name": "remcycle",
  "version": "0.2.0-beta.7",
  "description": "a Cycle.js-based component factory and set of higher-order-components built on cycle-react-driver, cycle-redux-driver, and most",
  "author": "Sunny G <sunny.gonna@gmail.com> (http://blog.sunnyg.io)",
  "homepage": "https://github.com/sunny-g/remcycle",
  "repository": "https://github.com/sunny-g/remcycle",
  "bugs": "https://github.com/sunny-g/remcycle/issues",
  "license": "ISC",
  "main": "commonjs/remcycle.js",
  "module": "es2015/index.js",
  "types": "es2015/index.d.ts",
  "files": [
    "commonjs",
    "es2015",
    "src",
    "test"
  ],
  "keywords": [
    "cycle",
    "react",
    "redux",
    "mostjs"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm test && tsc && npm run build -- -p",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/*.test.(ts|js)",
      "**/test/hoc/*.test.(ts|js)"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "transform": {
      "^.+\\.js": "<rootDir>/node_modules/babel-jest",
      "^.+\\.ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@sunny-g/cycle-utils/es2015))/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-dom/test-utils"
    ]
  },
  "dependencies": {
    "@cycle/isolate": "^2.1.0",
    "@most/hold": "^2.0.0",
    "@motorcycle/collection": "^1.1.0",
    "@sunny-g/cycle-react-driver": "^0.4.5",
    "@sunny-g/cycle-redux-driver": "^0.1.9",
    "@sunny-g/cycle-utils": "^0.2.0",
    "@types/node": "^6.0.69",
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "immutable": "^3.8.1",
    "most": "^1.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "shallow-equals": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "most-proxy": "^3.3.0",
    "ts-jest": "^19.0.1",
    "ts-loader": "^2.0.1",
    "typescript": "^2.3.0",
    "webpack": "^2.3.2"
  },
  "peerDependencies": {
    "@cycle/most-run": "^7.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
